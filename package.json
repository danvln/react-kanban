{
  "name": "@o2xp/react-kanban",
  "description": "@o2xp/react-kanban is a kanban with smooth drag and drop",
  "keywords": [
    "react",
    "component",
    "kanban",
    "atlassian",
    "atlassian/react-beautiful-dnd",
    "react-beautiful-dnd",
    "drag-and-drop",
    "dnd",
    "drag"
  ],
  "homepage": "https://github.com/o2xp/react-kanban",
  "bugs": "https://github.com/o2xp/react-kanban/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/o2xp/react-kanban"
  },
  "version": "1.0.0",
  "dependencies": {
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.55",
    "@material-ui/pickers": "^3.2.2",
    "chartist": "^0.11.4",
    "clsx": "^1.0.4",
    "copy-to-clipboard": "^3.2.0",
    "fast-deep-equal": "^3.1.1",
    "fuse.js": "^3.4.6",
    "js-cookie": "^2.2.1",
    "mdi-material-ui": "^6.8.0",
    "notistack": "^0.8.9",
    "react": "^16.8.6",
    "react-beautiful-dnd": "^12.1.1",
    "react-chartist": "^0.13.3",
    "react-dom": "^16.8.6",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.3",
    "react-spinners": "^0.6.1",
    "react-swipeable-views": "^0.13.9",
    "react-text-loop": "^2.3.0",
    "react-window": "^1.8.5",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "flow": "flow",
    "start": "react-scripts start",
    "build": "webpack --mode production",
    "test": "react-scripts test --coverage --watchAll=false --silent",
    "testDev": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.{js,jsx} --fix",
    "prettier": "prettier --write ./src/**/*.{js,jsx}",
    "version": "echo $npm_package_version",
    "storybook": "start-storybook -p 3000",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint ./src/**/*.{js,jsx} --fix",
      "prettier --write ./src/**/*.{js,jsx}",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/index.{js,jsx}",
      "!src/index.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@dump247/storybook-state": "^1.6.1",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-notes": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-mediaquery": "^0.1.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "flow-bin": "^0.107.0",
    "husky": "^2.7.0",
    "jest-canvas-mock": "^2.1.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  }
}
